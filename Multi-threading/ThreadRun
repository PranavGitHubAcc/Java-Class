
public class ThreadRun {
	public static void main(String[] args) throws Exception{
		int n = 5;
		for(int i = 0; i<n;i++) {
			ThreadDemo object = new ThreadDemo();
			object.start();
			Thread.sleep(1000); // 
		}
		System.out.println("Main Thread ID- "+ Thread.currentThread().threadId()); // main itself is a thread
	}
}
